<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qijiayiju.qijiayiju_v2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.qijiayiju.qijiayiju_v2.MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.qijiayiju.qijiayiju_v2.PopActivity"
            android:label="@string/title_activity_pop" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.qijiayiju.activity.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.qijiayiju.qijiayiju_v2.LoadActivity"
            android:label="@string/title_activity_load" >
        </activity>
        <activity
            android:name="com.qijiayiju.qijiayiju_v2.NewDetailActivity"
            android:label="@string/title_activity_new_detail" >
        </activity>
        <activity
            android:name="com.qijiayiju.qijiayiju_v2.RecordActivity"
            android:label="@string/title_activity_record"
            android:theme="@android:style/Theme.Dialog" >
        </activity>

        

        <activity
            android:name="com.qijiayiju.qijiayiju_v2.TongfengAutoActivity"
            android:label="@string/title_activity_tongfeng_auto"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        
        <receiver 
            android:name="com.qijiayiju.qijiayiju_v2.MyReceiver" >
            <intent-filter>
                <action android:name="com.qijiayiju.qijiayiju_v2" />
            </intent-filter>
        </receiver>
    </application>

</manifest>
